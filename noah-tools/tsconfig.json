{
	"compilerOptions": {
		"target": "es6",
		"module": "ES2020",
		"lib": ["dom", "dom.iterable", "esnext"],
		"skipLibCheck": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"preserveConstEnums": true,
		"strictFunctionTypes": false,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"jsx": "react-jsx",
		"importHelpers": true,
		"noEmit": true
	}
}
